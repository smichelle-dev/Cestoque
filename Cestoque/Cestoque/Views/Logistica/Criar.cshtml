@model FornecedoresModel
@{
    ViewData["Title"] = "Cadastrar logistica";
}


<form asp-controller="" asp-action="" method="post">

    <div class="form-group">
        <label for="NomeFornecedor">Gasolina</label>
        <input type="text" class="form-control" placeholder="Digite o gasto que você teve com a gasolina!">


    </div>
    <div class="form-group">
        <label for="NomeProduto">Montagem</label>
        <input type="text" class="form-control" placeholder="Digite o gasto que você teve com a montagem!">


    </div>
    <div class="form-group">
        <label for="ValorUnd">Embalagem</label>
        <input type="text" class="form-control" placeholder="Digite o gasto que você teve com a embalagem do produto!">


    </div>
    <div class="form-group">
        <label for="Data">Flayers</label>
        <input type="date" class="form-control" placeholder="Digite o gasto que você teve com os flayers!">


    </div>
    <div class="form-group">
        <label for="Mes">Data</label>
        <input type="text" class="form-control" placeholder="Digite a data!">


    </div>
    <div class="form-group">
        <label for="Mes">Mes</label>
        <input type="text" class="form-control" placeholder="Digite o mês!">


    </div>
    <button type="submit" class="btn btn-primary btn-block">Adicionar</button>
    <a class="btn btn-secondary btn-block" asp-controller="Logistica" asp-action="Index" role="button">Voltar</a>
</form>
